<script>
    var personnel = <%= Person.pluck( :name).to_json.to_s.gsub('/', '\/').html_safe %>
    var companies = <%= Company.pluck( :name).to_json.to_s.gsub('/', '\/').html_safe %>
</script>
<script type="text/javascript">
    $(function () {
        $('#datetime_out').datetimepicker({
            format: 'l, LTS'
        });
        $("#company").autocomplete({
            source: companies
        });
        $("#person_visiting").autocomplete({
            source: personnel
        });
    });
</script>
